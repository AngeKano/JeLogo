{
  "expo": {
    "name": "JELOGO",
    "slug": "JELOGO",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/playstore2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/playstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#0372C1"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ange002.JELOGO"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/playstore2.png",
        "backgroundColor": "#0372C1"
      },
      "package": "com.ange002.JELOGO",
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a540496e-f489-4fc6-bb65-86572b73ddc8"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts."
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ]
    ]
  }
}


